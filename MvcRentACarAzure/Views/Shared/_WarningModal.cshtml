
<div class="modal fade" id="warningModal" tabindex="-1" role="dialog" aria-labelledby="warningModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header bg-warning">
				<h5 class="modal-title" id="warningModalLabel">
					<i class="fa-solid fa-circle-exclamation"></i> Confirmación
				</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			@if (TempData["WarningMessage"] == null)
			{
				<div class="modal-body text-center mt-3" style="height:80px">
					¿Seguro que deseas cerrar sesión?
				</div>
			}else{
				<div class="modal-body text-center mt-3" style="height:80px">
					@TempData["WarningMessage"]
				</div>
			}

			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
				@if (TempData["WarningMessageBtn"] == null)
				{
					<button type="button" class="btn btn-danger" id="confirmButton">Cerrar Sesión</button>
				}else
				{
					<button type="button" class="btn btn-danger" id="confirmButton">@TempData["WarningMessageBtn"]</button>

				}
			</div>
		</div>
	</div>
</div>
